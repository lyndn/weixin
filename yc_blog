/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `wxuser` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) NOT NULL COMMENT '用户ID',
  `wxname` varchar(60) NOT NULL COMMENT '公众号名称',
  `wxid` varchar(20) NOT NULL COMMENT '公众号原始ID',
  `weixin` char(20) NOT NULL COMMENT '微信号',
  `headerpic` char(255) DEFAULT NULL COMMENT '头像地址',
  `token` char(255) NOT NULL COMMENT 'TOKEN',
  `typeid` int(11) NOT NULL COMMENT '分类ID',
  `typename` varchar(90) NOT NULL DEFAULT '0' COMMENT '分类名',
  `appid` varchar(50) NOT NULL DEFAULT '' COMMENT 'AppID',
  `appsecret` varchar(50) NOT NULL DEFAULT '' COMMENT 'AppSecret',
  `serverUrl` varchar(200) DEFAULT NULL COMMENT '服务URL',
  `EncodeType` smallint(3) DEFAULT NULL COMMENT '加密方式:1为明文，2为兼容，3为加密',
  `AesEncodingKey` char(32) DEFAULT NULL COMMENT '加密消息的AesEncodingKey',
  `qrcode` varchar(50) DEFAULT NULL COMMENT '二维码URL',
  `addtime` int(10) DEFAULT NULL COMMENT '添加时间',
  `mch_id` int(10) DEFAULT NULL COMMENT '支付号',
  `key` varchar(35) DEFAULT NULL COMMENT '支付密钥',
  `cert_path` varchar(50) DEFAULT NULL COMMENT '证书',
  `key_path` varchar(50) DEFAULT NULL COMMENT '支付证书',
  `operId` int(11) DEFAULT NULL COMMENT '操作员ID',
  PRIMARY KEY (`id`),
  KEY `uid` (`uid`),
  KEY `uid_2` (`uid`)
) ENGINE=MyISAM AUTO_INCREMENT=20 DEFAULT CHARSET=utf8 COMMENT='微信公众号表';
