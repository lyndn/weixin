  `cover` int(11) DEFAULT 1 COMMENT '是否显示封面 1 不显示  2 显示',
  `cntlink` int(11) DEFAULT 1 COMMENT '是否启用原文链接 1 不启用  2 启用',
  `current_url` mediumtext DEFAULT NULL COMMENT '封面图片当前url',
  `filetype` varchar(255) DEFAULT NULL COMMENT '文件类型',
  `filesize` varchar(255) DEFAULT NULL COMMENT '文件大小',
  `updatetime` datetime DEFAULT NULL COMMENT '上传时间',
  `current_dir_path` varchar(255) DEFAULT NULL COMMENT '当前系统目录',
  `synchronization` int(11) DEFAULT NULL COMMENT '是否同步 1 未同步 2 已同步',
  `has_file` int(11) DEFAULT NULL,
  `active` int(11) DEFAULT 0,
  `wechatid` int(11) DEFAULT NULL,
  `userid` int(11) DEFAULT NULL,
  `filename` mediumtext DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=20 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

