<script type="text/javascript">
    var isIE = (function() {
        var browser = navigator.appName;
        var b_version = navigator.appVersion;
        var version = b_version.split(";");
        version[1]=version[1]||"";
        var trim_Version = version[1].replace(/[ ]/g, "");
        var version;
        if (browser == "Microsoft Internet Explorer" && trim_Version == "MSIE6.0") {
            version = "IE6";
        } else if (browser == "Microsoft Internet Explorer" && trim_Version == "MSIE7.0") {
            version = "IE7";
        } else if (browser == "Microsoft Internet Explorer" && trim_Version == "MSIE8.0") {
            version = "IE8";
        } else if (browser == "Microsoft Internet Explorer" && trim_Version == "MSIE9.0") {
            version = "IE9";
        }
        return {
            version: version
        }
    })();
    if(isIE.version=='IE7'||isIE.version=='IE8'||isIE.version=="IE6"){
        $(function(){
            $('body').html('<p style="background:#fff;font-size:18px;line-height:94px;height:94px;text-align:center;">温馨提示：系统检测到您当前浏览器版本过低，建议使用 360 (极速模式)，Chrome 版本的浏览器 ,以获得更好的体验</p>');

        })
    }
    else
    {


        window.onbeforeunload = function() {
            if(!wxbEditor.hasSaved){
                return '还未保存素材，是否保存？';
            }
        }

        $(".user-invite-copy-full").on("click", function(){
            wxbEditor.mainUE.setContentSilent('')
        });



        $(window).resize(function () {
            var wrapWidth=$('.form-content-wrap').width();
            $('#editor>div').width(wrapWidth);
        });
        function checkLogin() {
            if (wxbEditor.userId == 0) {
                wxbEditor.store.showLogin();
                return false;
            }
            return true
        }
    }
</script>
